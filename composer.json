{
  "name": "ot-tracking-php",
  "description": "A PHP library for connecting with multiple carriers (FedEx, UPS, USPS) using Shippo.",
  "keywords": [
    "ups",
    "usps",
    "fedex",
    "dhl",
    "uber",
    "shyp",
    "shipping",
    "tracking",
    "address"
  ],
  "require": {
    "php": ">=5.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "google/apiclient": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
  },
  "autoload": {
    "classmap": ["lib/Shippo/"]
  }
}
